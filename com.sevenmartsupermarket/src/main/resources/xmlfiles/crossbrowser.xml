<suite name="CrossBrowserTest">
	<listeners>
		<listener class-name="com.sevenmartsupermarket.listeners.ExtentReporter"></listener>

	</listeners>
	<test name="Executing edge">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
	<test name="Executing Firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
		</classes>

	</test>
	<test name="Executing chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
</suite>